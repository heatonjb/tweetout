// Your custom SCSS should be written here...

  html,body {
    background-color: #ccc;
    background-image: url(../img/tweetout_web_1280.png);
    background-repeat: no-repeat;
    background-position: center 19px;
    background-size: cover;
    width: 100%;
    height: 100%;
  }



#bg {
  position: fixed; 
  top: 0; 
  left: 0; 

  /* Preserve aspet ratio */
  min-width: 100%;
  min-height: 100%;
}


  .column, .columns, .tiles > li {
    
 
    -moz-transition-duration: .3s;
    -o-transition-duration: .3s;
    -ms-transition-duration: .3s;
    transition-duration: .3s;
  }

  .column:hover, .columns:hover {

    cursor: pointer;
    -moz-transition-duration: .3s;
    -o-transition-duration: .3s;
    -ms-transition-duration: .3s;
    transition-duration: .3s;
  }

  .row {
    margin-bottom: 1px;
    text-align: center;
    background-color : #ffffff;
  }

  .row .tweet-header{
    position: relative;
    background-color:$tweet-header;
  }

  .row.new .tweet-header{
    background-color:$tweet-header-new;
  }

  .row.header, .row.controls {
    background-color: transparent;
    height: 50px;
  }

  .row.controls .two.columns {
    text-align: right;
  }

  h1, h2 {
    font: bold italic 60px georgia, times new roman, serif;
    color: #fff;
    letter-spacing: -.09em;
  }

  h1 a {
  	font: bold italic 60px georgia, times new roman, serif;
  	color: #fff;
  	letter-spacing: -2px;
  }

  h1 a:hover {
  	font: bold italic 60px georgia, times new roman, serif;
  	color: #DDDDDD;
  }

  h1.columns {
  	border: 0px;
  }

  h2  {
    font-family: helvetica;
    font-size: 33px;
    font-style: normal;
    font-weight: bold;
    letter-spacing: 0.005rem;
    text-align: right;
  
    a, a:hover {
      color: #fff;
      text-decoration: none;
    }
  }



  p {
    font-size: 16px;
    line-height: 30px;
    margin-bottom: 0;
    color: #555;

    -moz-transition-duration: .2s;
    -o-transition-duration: .2s;
    -ms-transition-duration: .2s;
    transition-duration: .2s;
  }

  .column:hover p, .columns:hover p {
    
    color: #000;

    -moz-transition-duration: .2s;
    -o-transition-duration: .2s;
    -ms-transition-duration: .2s;
    transition-duration: .2s;
  }

  .head, .head:hover {
    box-shadow: none;
    margin-top: 30px;
  }

  .head span {
    font-weight: bold;
    color: #353535;
  }

  .head span span {
    font-size: 32px;
    font-weight: bold !important;
  }

  a {
    color: #d04526;
  }

  a:hover {
    text-decoration:underline;
  }

  h1 a:hover {
    text-decoration:none;
  }

  .special, .special:hover {
    background: transparent;
        border: none;
  }

  .special:hover p {
    color: #ffe400;
  }



  .tweetrow{
  	min-width:320px;
    margin-top: 20px;
    position: relative;
  }

  .tweetrow.playing {
    border: 1px black solid;
  }

  #search {
  	font-size: 20px;
    height: 46px;
    padding-left: 10px;
    text-align: left;
    width: 100%;
    border: none;
   
  }

  .searchicon {
    background: url("../img/search_icon.png") no-repeat scroll center center transparent;
    bottom: 4px;
    display: block;
    height: 40px;
    position: absolute;
    right: 11px;
    width: 40px;
  }


.searchfield {
  width: 250px; 
  line-height:16px; 
  padding: 0px; 
  font-family:arial, sans-serif; 
  border: 0px solid #CCC; 
  border-radius: 2px; 
  -moz-border-radius: 2px; 
