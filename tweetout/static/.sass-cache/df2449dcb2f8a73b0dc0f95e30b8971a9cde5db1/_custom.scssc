/* MODAL WINDOWS */

body {
  /* jank buster */
  -webkit-transform: translateZ(0);
}

.modal {
  position: absolute;
  width: 330px;
  height: 277px;
  left: 50%;
  top: 100px;
  margin-left: -125px;
  padding: 1em;
  opacity: 0;
  -moz-transition: 0.2s;
  -ms-transition: 0.2s;
  transition: 0.2s;
  z-index:9999;
  pointer-events: none;
  color: #fff;
}

.modal.from-above {
  -webkit-transform: scale(2);
  -moz-transform: scale(2);
  -ms-transform: scale(2);
  transform: scale(2);
}

.modal.from-above.show {
  opacity: 1;
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
  box-shadow: 0 0 300px rgba(0, 0, 0, 0.3);
  
  pointer-events: auto;
}

.modal p, .modal h3 {
	color: #fff;
}


.modal.from-below {
  -webkit-transform: scale(0.5);
  -moz-transform: scale(0.5);
  -ms-transform: scale(0.5);
  transform: scale(0.5);
}

.modal.from-below.show {
  opacity: 1;
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
  box-shadow: 0 0 300px rgba(0, 0, 0, 0.3);
  
  pointer-events: auto;
}

.navbar a.toggle {
    top: 8%;
}

